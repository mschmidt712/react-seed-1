{
  "name": "react-seed",
  "version": "0.1.0",
  "description": "This is the seed repo for FrontEnd project for Kenzan.com - Linux.com learning series.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --config ./webpack.config.prod.js -p --progress",
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js --progress",
    "lint": "eslint *.js **/**/*.spec.jsx",
    "serve": "npm run build && http-server ./build -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenzanlabs/react-seed.git"
  },
  "keywords": [
    "kenzanlabs"
  ],
  "author": "rhernandez@kenzan.com",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-env": "^1.5.2",
    "eslint": "^4.1.1",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "rimraf": "^2.6.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  }
}
